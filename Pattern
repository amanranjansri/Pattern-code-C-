//  1st code

#include <iostream>
using namespace std;

int main()
{
    int a;
    cout<< "Enter the number :- ";
    cin>> a;

    int i = 1;

    while (i<=a)
    {
        int j = 1;
        while (j<=a)
        {
            cout<< "*";
            j = j+1;
        }
        cout<<endl;

        i = i+1;
    }

}





//2nd code 







#include <iostream>
using namespace std;

int main()
{
    int a;
    cout << "Enter the number : ";
    cin >> a;

    int i = 1;
    while (i <= a)
    {
        int j = 1;
        while (j <= a)
        {
            cout << i;
            j = j + 1;
        }
        cout << endl;
        i = i + 1;
    }
}






//3rd code







#include <iostream>
using namespace std;

int main()
{
    int a;
    cout << "Enter the number :- ";
    cin >> a;

    int i = 1;
    while (i <= a)
    {
        int j = 1;
        while (j <= a)
        {

            cout << j;
            j = j + 1;
        }
        cout << endl;
        i = i + 1;
    }
}






//4th code







#include <iostream>
using namespace std;

int main()
{
    int a;
    cout << "Enter the number : ";
    cin >> a;

    int i = 1;
    int count = 1;
    while (i <= a)
    {
        int j = 1;
        while (j <= a)
        {
            cout << count;
            count = count + 1;
            j = j + 1;
        }
        cout << endl;
        i = i + 1;
    }
}






