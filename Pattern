//7th code







#include <iostream>
using namespace std;

int main()
{
    int a;
    int count = 1;
    cout << "Enter the number : ";
    cin >> a;

    int i = 1;
    while (i <= a)
    {
        int j = 1;
        while (j <= i)
        {
            cout << count;
            count = count + 1;
            j = j + 1;
        }
        cout << endl;
        i = i + 1;
    }
}





//8th code








#include <iostream>
using namespace std;

int main()
{
    int a;
    cout << "Enter the value : ";
    cin >> a;

    int i = 1;

    while (i <= a)
    {
        int j = 1;
        int count = i;
        while (j <= i)
        {
            cout << count;
            count = count + 1;
            j = j + 1;
        }
        cout << endl;
        i = i + 1;
    }
}






//9th code







#include <iostream>
using namespace std;

int main()
{
    int a;
    cout<<"Enter the number : ";
    cin>> a;

    int i = 1;
    
    while(i <= a){
        if(i == 1) {cout<<"1\n" ; ++i ; continue ; }
        int count = 1 , index = i ; 
        while(count <= i){
            cout<<index<<" " ; 
            ++index ; ++count ; 
        } 
        cout<<endl ; 
        ++i ; 
    }
    
}
